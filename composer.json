{
    "name": "serkansyalcin/database-backup-manager",
    "description": "A Laravel package for managing database backups.",
    "autoload": {
        "psr-4": {
            "DatabaseBackupManager\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Serkan Syalcin",
            "email": "serkan.syalcin@hotmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DatabaseBackupManager\\Providers\\BackupServiceProvider"
            ]
        }
    },
    "keywords": ["laravel", "database", "backup", "manager", "csv", "json", "xml"],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true
